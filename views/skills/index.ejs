<!DOCTYPE html>
<html>
  <head>
      <link rel='stylesheet' href='/stylesheets/style.css' />
      <title>Express Index</title>
  </head>
  <body>
    <h1>All Skills</h1>
    <a href="/">Home</a>
    <ul>
        <% skills.forEach(function(s) { %>
            <li>
                <a href="/skills/<%= s.id %>"><%= s.skill %></a>
                  -
                <%= s.skillLevel %>
            </li>
        <% }); %>
    </ul>
  </body>
</html>
